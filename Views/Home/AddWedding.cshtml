@model Wedding
<h1>Plan Your Wedding</h1>
<form asp-action="CreateWedding">
    <div class="form-group">
        <label asp-for="WedderOne"></label>
        <input class="form-control" asp-for="WedderOne">
        <span asp-validation-for="WedderOne"></span>
    </div>
    <div class="form-group">
        <label asp-for="WedderTwo"></label>
        <input class="form-control" asp-for="WedderTwo">
        <span asp-validation-for="WedderTwo"></span>
    </div>
    <div class="form-group">
        <label asp-for="Date"></label>
        <input class="form-control" asp-for="Date">
        <span asp-validation-for="Date"></span>
    </div>
    <div class="form-group">
        <label asp-for="Address"></label>
        <input class="form-control" asp-for="Address">
        <span asp-validation-for="Address"></span>
    </div>
    <button class="btn btn-primary">Create</button>
</form>